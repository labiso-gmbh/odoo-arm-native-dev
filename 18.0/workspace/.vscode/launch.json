{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Odoo EE",
            "type": "debugpy",
            "request": "launch",
            "program": "/usr/bin/odoo-dev-bin",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                // "shell",
                "--exclude-addon-dirs=*template*,kronberg",
                "--database=dev_ee",
                "--log-level=info",
                "--log-handler=odoo.tools.convert:DEBUG",
                "--config=${workspaceFolder}/.vscode/odoo.conf",
                "--dev=qweb,xml",
                // "--load=base,web",
                "--init=norva24_germany_auto_partner_number",
                // "--test-enable",
                // "--test-tags=labidoo_project_access_rights",
                // "--stop-after-init",
                "--load-language=de_DE",
                "--language=de_DE",
                // "--i18n-export=${workspaceFolder}/de.po",
                // "--modules=labidoo_dms_sales_project"
            ]
        },
        {
            "name": "Odoo CE",
            "type": "debugpy",
            "request": "launch",
            "program": "/usr/bin/odoo-dev-bin",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                // "shell",
                "--exclude-addon-dirs=*enterprise*,*template*,",
                "--database=dev_ce",
                "--log-level=test",
                "--log-handler=odoo.tools.convert:DEBUG",
                "--config=${workspaceFolder}/.vscode/odoo.conf",
                "--dev=qweb,xml",
                "--load=base,web",
                "--init=web_editor_media_dialog_dms",
                // "--test-enable",
                // "--test-tags=/",
                // "--stop-after-init",
                "--load-language=de_DE",
                "--language=de_DE",
                // "--i18n-export=${workspaceFolder}/de.po",
                // "--modules=kachur_sale"
            ]
        },
    {
        "name": "Python: Current File",
        "type": "debugpy",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "justMyCode": false,
        "args": [
            "--customer=Norva24"
        ]
    }
    ],
}

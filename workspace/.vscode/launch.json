{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Odoo EE",
            "type": "debugpy",
            "request": "launch",
            "program": "/usr/bin/odoo-dev-bin",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                // "shell",
                "--exclude-addon-dirs=*template*",
                "--database=dev_ee",
                "--log-level=test",
                "--log-handler=odoo.tools.convert:DEBUG",
                "--log-handler=odoo.addons.labidoo_iot_report_printing.controllers.main:DEBUG",
                "--config=${workspaceFolder}/.vscode/odoo.conf",
                "--dev=qweb,xml",
                "--load=base,web",
                "--init=labidoo_mail",
                // "--update=labidoo_mail",
                // "--test-enable",
                // "--test-tags=labidoo_mail_recipients",
                // "--stop-after-init",
                "--load-language=de_DE",
                "--language=de_DE",
                // "--i18n-export=${workspaceFolder}/de.po",
                // "--modules=mq_event_ticket_management"
            ]
        },
        {
            "name": "Odoo CE",
            "type": "debugpy",
            "request": "launch",
            "program": "/usr/bin/odoo-dev-bin",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                // "shell",
                "--exclude-addon-dirs=*enterprise*,*template*",
                "--database=dev_ce",
                "--log-level=test",
                "--log-handler=odoo.tools.convert:DEBUG",
                "--config=${workspaceFolder}/.vscode/odoo.conf",
                "--dev=qweb,xml",
                "--load=base,web",
                // "--init=/",
                // "--test-enable",
                // "--test-tags=/",
                // "--stop-after-init",
                "--load-language=de_DE",
                "--language=de_DE",
                // "--i18n-export=${workspaceFolder}/de.po",
                // "--modules=/"
            ]
        },

        {
            "name": "Migration Script",
            "type": "python",
            "request": "launch",
            "program": "/workspace/rz-has/scripts/migraton_from_isp_billing.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-d",
                "dev_ee",
                "-c",
                "/workspace/.vscode/odoo.conf",
                "--customers-csv",
                "/workspace/rz-has/scripts/customers.csv",
                "--subscriptions-csv",
                "/workspace/rz-has/scripts/sale_subscriptions.csv",
            ]
        }
    ]
}